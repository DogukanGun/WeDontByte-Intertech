<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.IntertechAccount" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:fontFamily">@font/open_sans</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/intertech_actionbar_bottomnav_back_color</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/intertech_bottomnav_item_color</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/intertech_bottomnav_item_color</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/intertech_actionbar_bottomnav_back_color</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.IntertechAccount.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ThemeOverlay.IntertechAccount.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_900</item>
        <item name="fullscreenTextColor">@color/light_blue_A400</item>
    </style>
</resources>